{"ast":null,"code":"import { mapState, mapActions } from 'vuex';\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      submitted: false\n    };\n  },\n  computed: {\n    ...mapState('account', ['status'])\n  },\n  created() {\n    // reset login status\n    this.logout();\n  },\n  methods: {\n    ...mapActions('account', ['login', 'logout']),\n    handleSubmit(e) {\n      this.submitted = true;\n      const {\n        username,\n        password\n      } = this;\n      if (username && password) {\n        this.login({\n          username,\n          password\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapActions","data","username","password","submitted","computed","created","logout","methods","handleSubmit","e","login"],"sources":["C:\\Users\\tomda\\Documents\\Cours S4\\discord-vuejs\\Front\\src\\views\\Signin\\Signin.vue"],"sourcesContent":["<template>\r\n  <body class=\"bg-image\">\r\n    <div class=\"container w-50 position-absolute top-50 start-50 translate-middle\">\r\n      <div id=\"form_frame\" class=\"p-4\">\r\n        <h2 class=\"text-white\">Connexion</h2>\r\n          <form @submit.prevent=\"handleSubmit\">\r\n\r\n            <div class=\"form-outline mb-4\">\r\n              <label class=\"form-label text-light\" for=\"username\">Nom d'utilisateur</label>\r\n              <input v-model=\"username\" name=\"username\" type=\"text\" id=\"username\" class=\"text-white form-control\" />\r\n            </div>\r\n          \r\n            <div class=\"form-outline mb-4\">\r\n              <label class=\"form-label text-light\" for=\"password\">Mot de passe</label>\r\n              <input v-model=\"password\" name=\"password\" type=\"password\" id=\"password\" class=\"text-white form-control\" />\r\n            </div>\r\n\r\n            <button type=\"submit\" class=\"btn text-light btn-block mb-1\">Connexion</button>\r\n            <h6 class=\"text-white pt-2\">Pas de compte ? <a href=\"/signup\">Inscription</a></h6>\r\n          </form>\r\n          <div id=\"form_error\" class=\"alert alert-danger d-none\" role=\"alert\">\r\n            Les informations saisies sont incorrectes\r\n          </div>  \r\n        </div>  \r\n      </div>\r\n\r\n</body>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapActions } from 'vuex'\r\n\r\nexport default {\r\n    data () {\r\n        return {\r\n            username: '',\r\n            password: '',\r\n            submitted: false\r\n        }\r\n    },\r\n    computed: {\r\n        ...mapState('account', ['status'])\r\n    },\r\n    created () {\r\n        // reset login status\r\n        this.logout();\r\n    },\r\n    methods: {\r\n        ...mapActions('account', ['login', 'logout']),\r\n        handleSubmit (e) {\r\n            this.submitted = true;\r\n            const { username, password } = this;\r\n            if (username && password) {\r\n                this.login({ username, password })\r\n            }\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style>\r\nbody {\r\n    background-image: url(\"../assets/signin-background.png\");\r\n    height: 100vh;\r\n}\r\n\r\n#form_frame {\r\n    background-color: #343A3F;\r\n}\r\n\r\nbutton {\r\n    background-color: #6E89E0 !important;\r\n}\r\n\r\n.bg-image{\r\n  background-size: cover;\r\n  background-position: center;\r\n  background-repeat: no-repeat;\r\n}\r\n</style>"],"mappings":"AA8BA,SAASA,QAAQ,EAAEC,UAAS,QAAS,MAAK;AAE1C,eAAe;EACXC,IAAGA,CAAA,EAAK;IACJ,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE;IACf;EACJ,CAAC;EACDC,QAAQ,EAAE;IACN,GAAGN,QAAQ,CAAC,SAAS,EAAE,CAAC,QAAQ,CAAC;EACrC,CAAC;EACDO,OAAMA,CAAA,EAAK;IACP;IACA,IAAI,CAACC,MAAM,EAAE;EACjB,CAAC;EACDC,OAAO,EAAE;IACL,GAAGR,UAAU,CAAC,SAAS,EAAE,CAAC,OAAO,EAAE,QAAQ,CAAC,CAAC;IAC7CS,YAAWA,CAAGC,CAAC,EAAE;MACb,IAAI,CAACN,SAAQ,GAAI,IAAI;MACrB,MAAM;QAAEF,QAAQ;QAAEC;MAAS,IAAI,IAAI;MACnC,IAAID,QAAO,IAAKC,QAAQ,EAAE;QACtB,IAAI,CAACQ,KAAK,CAAC;UAAET,QAAQ;UAAEC;QAAS,CAAC;MACrC;IACJ;E