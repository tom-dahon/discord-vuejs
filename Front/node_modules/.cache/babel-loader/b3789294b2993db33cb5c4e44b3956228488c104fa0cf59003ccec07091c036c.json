  let res = await fetch('/api/auth/signup',requestOptions)\r\n        .catch(err =>{\r\n          console.log(err);\r\n        });\r\n        if(res.status == 200) {\r\n          let infos = await res.json();\r\n        setCookie('token', infos.accessToken, 1);\r\n        setCookie('userId', infos.id, 1);\r\n        window.location.replace('../home/index.html');\r\n        } else if(res.status == 422) {\r\n            email.classList.add('border', 'border-danger');\r\n            email.focus();\r\n            email_error.classList.replace('d-none','d-block');\r\n        }\r\n      }\r\n\r\n</script>\r\n\r\n<style>\r\nbody {\r\n    background-image: url(\"../assets/signup-background.jpg\");\r\n    height: 100vh;\r\n}\r\n\r\n#form_frame {\r\n    background-color: #343A3F;\r\n}\r\n\r\nbutton {\r\n    background-color: #6E89E0 !important;\r\n}\r\n\r\n.bg-image{\r\n  background-size: cover;\r\n  background-position: center;\r\n  background-repeat: no-repeat;\r\n}\r\n</style>"],"mappings":"AAqCE,SAASA,QAAQ,QAAQ,MAAM;;;;;;;IAE/B,MAAMC,KAAK,GAAGD,QAAQ,EAAE;IAExB,eAAeE,MAAMA,CAACC,CAAC,EAAE;MACvBA,CAAC,CAACC,cAAc,EAAE;MAClB,MAAMC,IAAI,GAAG;QAAE,OAAO,EAAEC,KAAK,CAACC,KAAK;QAAE,UAAU,EAAEC,QAAQ,CAACD,KAAK;QAAE,UAAU,EAAEE,QAAQ,CAACF,KAAK;QAAE,QAAQ,EAAEG,IAAI,CAACH;MAAM,CAAC;MACnH,IAAII,OAAO,GAAG,IAAIC,OAAO,EAAE;MAC3BD,OAAO,CAACE,MAAM,CAAC,cAAc,EAAC,kBAAkB,CAAC;MACjD,IAAIC,cAAc,GAAG;QACjBC,MAAM,EAAE,MAAM;QACdJ,OAAO,EAAEA,OAAO;QAChBK,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACb,IAAI;MAC3B,CAAC;MAEH,IAAIc,GAAG,GAAG,MAAMC,KAAK,CAAC,kBAAkB,EAACN,cAAc,CAAC,CACnDO,KAAK,CAACC,GAAG,IAAG;QACXC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAClB,CAAC,CAAC;MACF,IAAGH,GAAG,CAACM,MAAM,IAAI,GAAG,EAAE;QACpB,IAAIC,KAAK,GAAG,MAAMP,GAAG,CAACQ,IAAI,EAAE;QAC9BC,SAAS,CAAC,OAAO,EAAEF,KAAK,CAACG,WAAW,EAAE,CAAC,CAAC;QACxCD,SAAS,CAAC,QAAQ,EAAEF,KAAK,CAACI,EAAE,EAAE,CAAC,CAAC;QAChCC,MAAM,CAACC,QAAQ,CAACC,OAAO,CAAC,oBAAoB,CAAC;MAC7C,CAAC,MAAM,IAAGd,GAAG,CAACM,MAAM,IAAI,GAAG,EAAE;QACzBnB,KAAK,CAAC4B,SAAS,CAACC,GAAG,CAAC,QAAQ,EAAE,eA